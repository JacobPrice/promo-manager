{
    "require-dev": {
        "hellonico/timber-dump-extension": "^1.0"
    },
    "require": {
        "timber/timber": "2.0.0-rc.1",
        "boxybird/morph": "^0.3.1",
        "lpd/og-plugin": "0.0.1",
        "fakerphp/faker": "^1.23",
        "htmlburger/carbon-fields-plugin": "^3.6"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "autoload": {
        "psr-4": {
            "LpdPromo\\": "app/",
            "OgPlugin\\": "OgPlugin/"
        }
    },
    "repositories": {
        "og-plugin": {
            "type": "path",
            "url": "./og-plugin"
        }
    }
}
